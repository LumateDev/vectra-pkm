{
  "Vectra": {
    "Environment": "Production",
    "Database": {
      "Host": "host.docker.internal",
      "Port": 5432,
      "Name": "vectra",
      "Username": "admin"
    },
    "Jwt": {
      "Issuer": "vectra-api",
      "Audience": "vectra-client",
      "ExpiryMinutes": 60
    },
    "Cors": {
      "AllowedOrigins": [
        "http://localhost:5173",
        "https://localhost:5173",
        "http://localhost:4173",
        "https://localhost:4173"
      ],
      "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
      "AllowedHeaders": ["Content-Type", "Authorization", "X-Requested-With"]
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080"
      },
      "Https": {
        "Url": "https://*:8081"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
